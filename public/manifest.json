{
  "manifest_version": 3,
  "name": "Endless Wallet",
  "version": "1.0",
  "description": "Login with Endless Wallet",
  "permissions": ["storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://apibot.luffa.im/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    },
    "default_title": "Endless Wallet"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
